[flake8]
max-line-length = 100
extend-ignore = E203, E266, E501, W503
exclude =
    .git,
    __pycache__,
    venv,
    env,
    .venv,
    build,
    dist,
    *.egg-info,
    .mypy_cache,
    .pytest_cache,
    .tox,
    unity/DreamWalkVR,
    models/checkpoints,
    models/exports
per-file-ignores =
    __init__.py:F401
max-complexity = 10

