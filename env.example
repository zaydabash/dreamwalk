# DreamWalk Environment Configuration

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
HUGGINGFACE_HUB_TOKEN=your_huggingface_token_here

# Database URLs
REDIS_URL=redis://localhost:6379
KAFKA_BROKER=localhost:9092

# Service URLs (for development)
SIGNAL_PROCESSOR_URL=http://localhost:8001
NEURAL_DECODER_URL=http://localhost:8002
REALTIME_SERVER_URL=http://localhost:8003
TEXTURE_GENERATOR_URL=http://localhost:8005
NARRATIVE_LAYER_URL=http://localhost:8006

# Model Configuration
MODEL_PATH=./models/checkpoints
OUTPUT_PATH=./unity/DreamWalkVR/Assets/Generated

# EEG Hardware Configuration
EEG_DEVICE_PORT=/dev/ttyUSB0
EEG_SAMPLING_RATE=250
EEG_CHANNELS=8

# Performance Settings
MAX_LATENCY_MS=100
PROCESSING_RATE_HZ=10
TEXTURE_GENERATION_TIMEOUT=30

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Development Settings
DEBUG_MODE=false
MOCK_EEG_ENABLED=true
ENABLE_PROFILING=false

# Unity Configuration
UNITY_PROJECT_PATH=./unity/DreamWalkVR
UNITY_BUILD_PATH=./unity/builds

# Monitoring
PROMETHEUS_PORT=9090
GRAFANA_PORT=3000
# WARNING: Change this password in production!
# Store in secure secret management system, not in environment files
GRAFANA_ADMIN_PASSWORD=changeme_in_production
